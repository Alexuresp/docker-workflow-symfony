{
    "name": "covex-nn/docker-symfony-se",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" for docker distribution",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5.11",
        "friendsofsymfony/user-bundle": "2.1.x-dev",
        "sensio/framework-extra-bundle": "^5.0",
        "sonata-project/admin-bundle": "dev-twig as 3.29.0.1",
        "sonata-project/block-bundle": "dev-twig as 3.9.0.1",
        "sonata-project/doctrine-orm-admin-bundle": "dev-twig as 3.3.0.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/debug-bundle": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/stopwatch": "^4.0",
        "symfony/web-profiler-bundle": "^4.0"
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/covex-nn/SonataBlockBundle" },
        { "type": "vcs", "url": "https://github.com/covex-nn/SonataAdminBundle" },
        { "type": "vcs", "url": "https://github.com/covex-nn/SonataDoctrineORMAdminBundle" }
    ],
    "config": {
        "platform": {
            "php": "7.2.0"
        },
        "sort-packages": true,
        "vendor-dir": "/composer/vendor"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "scripts": {
        "auto-scripts": {

        }
    }
}
